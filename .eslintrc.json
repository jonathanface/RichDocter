{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "max-len":"off",
      "comma-dangle":"off",
      "new-cap":"off",
      "brace-style": [2, "1tbs", { "allowSingleLine": true }]
    }
}
